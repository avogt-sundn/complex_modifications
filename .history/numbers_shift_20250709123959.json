{
    "title": "ANSI Backslash",
    "rules": [
        {
            "description": "ANSI Backslash",
            "manipulators": [
                // pipe
                {
                    "type": "basic",
                    "from": {
                        "key_code": "backslash",
                        "modifiers": {
                            "mandatory": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "7",
                            "modifiers": [
                                "left_option"
                            ]
                        }
                    ]
                },
                // ]
                {
                    "type": "basic",
                    "from": {
                        "key_code": "backslash"
                    },
                    "to": [
                        {
                            "key_code": "7",
                            "modifiers": [
                                "left_option",
                                "left_shift"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}