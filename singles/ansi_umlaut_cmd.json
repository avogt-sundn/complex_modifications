{
    "title": "ANSI Umlaute per CMD",
    "rules": [
        {
            "description": "ANSI Umlaute per CMD",
            "manipulators": [
                // CMD- -> umlaut s
                {
                    "type": "basic",
                    "from": {
                        "key_code": "slash",
                        "modifiers": {
                            "mandatory": [
                                "left_command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "hyphen"
                        }
                    ]
                },
                // CMD s -> umlaut s
                {
                    "type": "basic",
                    "from": {
                        "key_code": "s",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "hyphen"
                        }
                    ]
                },
                // CMD[ -> ü
                {
                    "type": "basic",
                    "from": {
                        "key_code": "open_bracket",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "open_bracket"
                        }
                    ]
                },
                // shift-CMD =_[ -> Ü
                {
                    "type": "basic",
                    "from": {
                        "key_code": "open_bracket",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "open_bracket",
                            "modifiers": [
                                "left_shift"
                            ]
                        }
                    ]
                },
                // CMD; -> ö
                {
                    "type": "basic",
                    "from": {
                        "key_code": "semicolon",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "semicolon"
                        }
                    ]
                },
                // CMD; -> Ö
                {
                    "type": "basic",
                    "from": {
                        "key_code": "semicolon",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "semicolon",
                            "modifiers": [
                                "left_shift"
                            ]
                        }
                    ]
                },
                // CMD; -> ä
                {
                    "type": "basic",
                    "from": {
                        "key_code": "quote",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "quote"
                        }
                    ]
                },
                // CMD; -> Ä
                {
                    "type": "basic",
                    "from": {
                        "key_code": "quote",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "quote",
                            "modifiers": [
                                "left_shift"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}