{
    "title": "ANSI function keys",
    "rules": [
        {
            "description": "ANSI function keys aktivieren",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "consumer_key_code": "display_brightness_increment"
                    },
                    "to": [
                        {
                            "key_code": "f2"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "consumer_key_code": "display_brightness_decrement"
                    },
                    "to": [
                        {
                            "key_code": "f1"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "spacebar"
                            },
                            {
                                "key_code": "left_command"
                            }
                        ]
                    },
                    "to": [
                        {
                            "key_code": "f4"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "consumer_key_code": "dictation"
                    },
                    "to": [
                        {
                            "key_code": "f5"
                        }
                    ]
                }
            ]
        }
    ]
}