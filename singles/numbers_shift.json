{
    "title": "ANSI Numbers Shift",
    "rules": [
        {
            "description": "ANSI Numbers Shift",
            "manipulators": [
                // 0 shift -> )
                {
                    "type": "basic",
                    "from": {
                        "key_code": "0",
                        "modifiers": {
                            "mandatory": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "9",
                            "modifiers": [
                                "shift"
                            ]
                        }
                    ]
                },
                // 9 shift -> (
                {
                    "type": "basic",
                    "from": {
                        "key_code": "9",
                        "modifiers": {
                            "mandatory": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "8",
                            "modifiers": [
                                "shift"
                            ]
                        }
                    ]
                },
                // 8 shift -> *
                {
                    "type": "basic",
                    "from": {
                        "key_code": "8",
                        "modifiers": {
                            "mandatory": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "close_bracket",
                            "modifiers": [
                                "shift"
                            ]
                        }
                    ]
                },
                // 7 shift -> &
                {
                    "type": "basic",
                    "from": {
                        "key_code": "7",
                        "modifiers": {
                            "mandatory": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "6",
                            "modifiers": [
                                "shift"
                            ]
                        }
                    ]
                },
                // 6 shift -> ^
                {
                    "type": "basic",
                    "from": {
                        "key_code": "6",
                        "modifiers": {
                            "mandatory": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "6",
                            "modifiers": [
                                "shift",
                                "option"
                            ]
                        }
                    ]
                },
                // 3 shift -> #
                {
                    "type": "basic",
                    "from": {
                        "key_code": "3",
                        "modifiers": {
                            "mandatory": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "backslash"
                        }
                    ]
                },
                // 2 shift -> @
                {
                    "type": "basic",
                    "from": {
                        "key_code": "2",
                        "modifiers": {
                            "mandatory": [
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "l",
                            "modifiers": [
                                "option"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}