{
    "title": "ANSI Undo Z",
    "rules": [
        {
            "description": "ANSI Undo Z",
            "manipulators": [
                // CMD Z
                {
                    "type": "basic",
                    "from": {
                        "key_code": "z",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "y",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ]
                },
                // CMD SHIFT Z
                {
                    "type": "basic",
                    "from": {
                        "key_code": "z",
                        "modifiers": {
                            "mandatory": [
                                "command",
                                "shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "y",
                            "modifiers": [
                                "command",
                                "shift"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}